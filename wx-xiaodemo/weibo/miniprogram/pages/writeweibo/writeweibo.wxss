.container{
    padding: 40rpx 60rpx;
}
.container .textarea{
 font-size: 28rpx;
 width: 100%;
 height: 200rpx;

}
.container .image-list-group{
    padding-top: 20rpx;
    display: flex;
    justify-content: flex-start;
    flex-wrap:wrap;
}
.image-list-group .image-group{
    margin-right: 5rpx;
    position: relative;
    margin-bottom: 5rpx;
}
.image-list-group .image-group image{
    width: 100%;
    height: 100%;
}
.image-list-group .image-group .close-btn{
    position: absolute;
    right: -30rpx;
    top: -30rpx;
background-repeat: no-repeat;
background-position: center;
    width: 60rpx;
    height: 60rpx;
    z-index: 1;
    background-size: 50rpx;
   
    background-image: url('data:image/png;base64,');
}
.image-list-group .add-image-btn{
    font-size:200rpx;
    background: #f2f2f2;
    color: #8b8888;
    display: flex;
    justify-content: center;
    align-items: center;

}
.add-image-btn view{
    margin-top: -20rpx;
    color: #5c5959;
}

.container .location-group{
    margin-top: 40rpx;
    border-top: 1px solid #e4e4e4;
   border-bottom: 1px solid #e4e4e4;
    padding: 30rpx 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.location-group .left-group{
   display: flex;
   font-size:28rpx;
   
}
.location-group .left-group .icon{
   width: 40rpx;
   height: 40rpx;
   margin-right: 40rpx;
}
.location-group .arrow{
    width: 40rpx;
    height: 40rpx;
}
.submit-btn{
   margin-top: 40rpx;
}